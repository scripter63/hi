local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "wowsers hub Universal", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

OrionLib:MakeNotification({
	Name = "hacks :O!",
	Content = "for the hecers",
	Image = "rbxassetid://4483345998",
	Time = 5
})

local Tab = Window:MakeTab({
	Name = "universal",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddButton({
	Name = "trolling gui",
	Callback = function()
        loadstring(game:HttpGet("https://scriptblox.com/raw/Universal-Script-FE-Trolling-GUI-10739"))()
  	end    
})

Tab:AddButton({
	Name = "hairfling",
	Callback = function()
        loadstring(game:HttpGet("https://scriptblox.com/raw/Universal-Script-FE-hair-fling-1702"))()
        end


